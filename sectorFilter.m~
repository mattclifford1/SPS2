function Q = sectorFilter(Q,center,theta1,theta2)
% close all
% figure
% subplot(1,2,1);
% imagesc(log(abs(q)+1)); 
dim = size(Q);
center = (dim/2) + center;
for i = 1:dim(1)
    for j = 1:dim(2)
        if atan((j-center(2))/(i-center(1))) > theta1 ...
                && atan((j-center(2))/(i-center(1))) < theta2 ...
                && i^2 + j^2 < 
            Q(i,j) = Q(i,j);
        else
            Q(i,j) = 0;
        end
    end
end
% subplot(1,2,2);
% imagesc(log(abs(q)+1)); 
